{
  "name": "interviewai-prepbot",
  "version": "1.0.0",
  "description": "AI-powered interview preparation platform with frontend and backend",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm start",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "test": "echo \"No tests specified\" && exit 1"
  },
  "keywords": [
    "interview",
    "ai",
    "preparation",
    "react",
    "nodejs",
    "supabase",
    "vite"
  ],
  "author": "Kamaal Alag",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Kamaal2002/interviewai-prepbot.git"
  },
  "bugs": {
    "url": "https://github.com/Kamaal2002/interviewai-prepbot/issues"
  },
  "homepage": "https://github.com/Kamaal2002/interviewai-prepbot#readme"
} 